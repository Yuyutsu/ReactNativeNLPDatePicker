{
  "name": "react-native-nlp-calendar",
  "version": "1.0.0",
  "description": "A reusable React Native package that accepts natural language text, parses dates using an NLP layer, and renders them as structured calendar events.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "tsc",
    "test": "jest",
    "lint": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Yuyutsu/ReactNativeNLPDatePicker.git"
  },
  "homepage": "https://github.com/Yuyutsu/ReactNativeNLPDatePicker#readme",
  "bugs": {
    "url": "https://github.com/Yuyutsu/ReactNativeNLPDatePicker/issues"
  },
  "author": "Yuyutsu",
  "license": "MIT",
  "keywords": [
    "react-native",
    "nlp",
    "calendar",
    "date-parser",
    "natural-language",
    "typescript",
    "ios",
    "android"
  ],
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "react": ">=18.3.0",
    "react-native": ">=0.73.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.24.0",
    "@babel/preset-typescript": "^7.24.0",
    "@testing-library/react-native": "^12.4.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.3.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "react": "18.3.1",
    "react-native": "0.76.5",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.(ts|tsx)"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|@testing-library)/)"
    ]
  }
}
